{
  "name": "financial-tracker",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "pretty": "prettier --write src/**/*.ts src/**/*.tsx",
    "lint": "yarn run tslint --project ./",
    "a11y:axe": "axe http://localhost:3000 --tags wcag2a,wcag2aa,best-practice --browser chrome",
    "a11y:pa11y": "pa11y http://localhost:3000 --standard WCAG2AA",
    "a11y": "concurrently --names \"a11y:axe,a11y:pa11y\" --prefix-colors \"blue,green\" \"yarn run a11y:axe\" \"yarn run a11y:pa11y\"",
    "test": "jest",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "blob": "^0.0.5",
    "chart.js": "^2.8.0",
    "connected-react-router": "^6.2.2",
    "firebase": "^5.8.2",
    "moment": "^2.24.0",
    "react": "^16.8.0",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^4.0.8",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/chart.js": "^2.7.52",
    "@types/classnames": "^2.2.7",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.21",
    "@types/react": "^16.7.18",
    "@types/react-chartjs-2": "^2.5.7",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/recompose": "^0.30.5",
    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "axe-cli": "^3.0.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "pa11y": "^5.1.0",
    "prettier": "^1.16.4",
    "react-swipeable-views": "^0.13.1",
    "tslint": "^5.13.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.3.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
