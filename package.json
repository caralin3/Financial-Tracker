{
  "name": "financial-tracker",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "5.0.19",
    "@types/recompose": "^0.26.4",
    "@types/redux": "^3.6.0",
    "firebase": "^5.3.0",
    "react": "^16.4.2",
    "react-csv": "^1.0.18",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-vis": "^1.11.4",
    "recompose": "^0.28.2",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && yarn run build:css",
    "build:css": "lessc src/less/index.less public/styles.css",
    "deploy": "yarn run build && firebase deploy",
    "test": "jest",
    "watch:css": "chokidar \"**/*.less\" -c \"npm run build:css\"",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.5",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "jest": "^23.4.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "^2.13.5",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
